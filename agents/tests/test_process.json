      "type": "start",
      "name": "Start Process",
      "description": "Order Received",
      "properties": {
        "position": {"x": 100, "y": 150}
      }
    },
    {
      "id": "verify_order",
      "type": "task",
      "name": "Verify Order",
      "description": "Check order details and payment",
      "properties": {
        "duration": 5,
        "position": {"x": 250, "y": 150},
        "system": "OrderSystem"
      }
    },
    {
      "id": "check_inventory",
      "type": "task",
      "name": "Check Inventory",
      "description": "Check if items are in stock",
      "properties": {
        "duration": 5,
        "position": {"x": 400, "y": 150},
        "system": "InventorySystem"
      }
    },
    {
      "id": "decision_1",
      "type": "gateway",
      "name": "Items Available?",
      "description": "Check if all items are available",
      "properties": {
        "gateway_type": "exclusive",
        "position": {"x": 550, "y": 150}
      }
    },
    {
      "id": "notify_backorder",
      "type": "task",
      "name": "Create Backorder",
      "description": "Create backorder for out-of-stock items",
      "properties": {
        "duration": 10,
        "position": {"x": 550, "y": 300},
        "system": "InventorySystem"
      }
    },
    {
      "id": "process_payment",
      "type": "task",
      "name": "Process Payment",
      "description": "Process customer payment",
      "properties": {
        "duration": 15,
        "position": {"x": 700, "y": 150},
        "system": "PaymentSystem"
      }
    },
    {
      "id": "prepare_shipment",
      "type": "task",
